<script src="/javascripts/retina.min.js"></script>

<!-- include accordion / tabs plugin -->
<script src="/javascripts/whistles.min.js"></script>
<!-- include mousewheel plugins -->
<script src="/javascripts/jquery.mousewheel.min.js"></script>
<% unless notice.nil? or notice.empty? %>
	<script type="text/javascript">
		toastr['success']('<%= notice %>');
	</script>
<% end %>
<% unless alert.nil? or alert.empty? %>
	<script type="text/javascript">
	console.log('<%=alert%>');
		toastr['error']('<%= alert %>');
	</script>
<% end %>
