<% unless notice.nil? or notice.empty? %>
	<script type="text/javascript">
		toastr['success']('<%= notice %>');
	</script>
<% end %>
<% unless alert.nil? or alert.empty? %>
	<script type="text/javascript">
	console.log('<%=alert%>');
		toastr['error']('<%= alert %>');
	</script>
<% end %>
